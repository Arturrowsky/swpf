@model ArturCzarnecki_9807.Godziny
@{
    ViewBag.Title = "UsunGodziny";
    Layout = "~/Views/Layouts/FizjoterapeutaLayout.cshtml";
}


@if (Session["User_ID"] != null)
{
    <div class="col" style="margin-left: 30%; text-align: center;">
        <h3>Czy napewno chcesz usunąć tę godzinę z Twojego harmonogramu?</h3>
        <div>
         
            <dl class="dl-horizontal">
                <dd>
                    @Html.DisplayFor(model => model.Godzina)
                </dd>
            </dl>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <div class="form-actions no-color">
                    <input type="submit" value="Usuń" class="button" />
                </div>
            }
        </div>
        </div>
        }
